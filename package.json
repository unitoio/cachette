{
  "name": "cachette",
  "version": "0.1.0",
  "engines": {
    "node": ">=10.14.1",
    "npm": ">=6.4.1"
  },
  "description": "Resilient cache library supporting concurrent requests through local cache or Redis.",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "repository": "https://github.com/unitoio/cachette",
  "author": {
    "name": "Unito",
    "email": "hello@unito.io"
  },
  "license": "MIT",
  "keywords": [
    "typescript",
    "cache",
    "concurrent",
    "redis",
    "decorator"
  ],
  "scripts": {
    "clean": "rm -rf dist/ node_modules/ shippable/ coverage/ *.tgz",
    "compile": "tsc",
    "prepublishOnly": "npm run clean && npm install && npm run compile",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "test": "npm run compile && npm run coverage",
    "test:ci": "mocha --ui tdd -R xunit > shippable/testresults/result.xml",
    "coverage": "nyc mocha"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.8",
    "@types/sinon": "^7.0.12",
    "chai": "^4.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "remap-istanbul": "^0.13.0",
    "sinon": "^7.3.2",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-no-unused-expression-chai": "0.1.4",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@types/bluebird": "^3.0.36",
    "@types/redlock": "^3.0.1",
    "bluebird": "^3.4.7",
    "lru-cache": "^5.1.1",
    "redis": "^2.6.3",
    "redlock": "^3.1.2"
  }
}
